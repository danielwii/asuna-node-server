  province              String?
  country               String?
  avatar                String?
  created_at            DateTime               @default(now())
  updated_at            DateTime               @default(now())
  updated_by            String?
  profile__id           String?                @unique
  auth__t_user_profiles auth__t_user_profiles? @relation(fields: [profile__id], references: [id])
