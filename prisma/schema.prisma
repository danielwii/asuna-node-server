  updated_by   String?
  message      String?
  body         Json?
  task__id     String?
  sys__t_tasks sys__t_tasks? @relation(fields: [task__id], references: [id])

  @@index([task__id], name: "FK_b9db76df8c6d282adb47c191808")
}

model sys__t_tenants {
